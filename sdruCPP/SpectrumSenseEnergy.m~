function decision = SpectrumSenseEnergy( ObjAGC, ObjSDRuReceiver, tx )

            
% Collect Data
N = 10;
data = complex(zeros(tx.receiveBufferLength,N));
for k = 1:N
    tmp = step(ObjSDRuReceiver);
    data(:,k) = step(ObjAGC,tmp);
end


meanEnergy = mean(mean(abs(fft(data(1:4096,:)))));

if meanEnergy > threshold
   decis 
end


end